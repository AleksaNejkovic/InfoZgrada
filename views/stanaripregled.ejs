    </div>
    <hr width="100%" style="background: gray">
    <div class="row justify-content-center">
        <a href="/novistanar" ><button type="button" class="btn btn-info">Dodaj stanara</button></a>
    </div>
    <hr width="100%" style="background: gray">
    <div class="row justify-content-center my-4">


          <center><h3>Podaci o stanarima</h3></center>

        <table class="table table-dark table-responsive-sm">
            <thead>
            <tr>
                <th scope="col">Ime</th>
                <th scope="col">Prezime</th>
                <th scope="col">Email</th>
                <th scope="col">Broj telefona</th>
                <th scope="col">Datum rođenja</th>
                <th scope="col">Broj licne karte</th>
                <th scope="col">Broj stana</th>
                <th scope="col">Akcija</th>

            </tr>
            </thead>
            <% data.forEach(function(x) { %>
            <tbody>

            <tr>

                <td><%=x.ime%></td>
                <td><%=x.prezime%></td>
                <td><%=x.email%></td>
                <td><%=x.broj_telefona%></td>
                <td><%=x.datum_rodjenja%></td>
                <td><%=x.broj_licne_karte%></td>
                <td><%=x.broj_stana%></td>



                <td><a href="/izmeni/stanara/<%=x.id%>" ><i class="fas fa-edit"></i></a>&nbsp;&nbsp;&nbsp;<a href="/izbrisi/stanara/<%=x.broj_stana%>/<%=x.id%>" ><i class="fas fa-trash-alt"></i></a></td>

            </tr>

            </tbody>
            <% }) %>
        </table>

    </div>

</div>
<script type="text/javascript" src="/jquery.js"></script>
